;Con // 必ず１文字
;数関係詞
[数字]
，	Type=Con	Read=，
．	Type=Con	Read=テン	Sound=テン
・	Type=Con	Read=・
;点	Type=Con	Read=・
点	Type=Con	Read=テン	Sound=テン
対	Type=Con	Read=タイ	Sound=タイ
秒	Type=Con	Read=ビョウ	Sound=ビョー
～	Type=Con	Read=カラ	Sound=カラ
－	Type=Con	Read=－
―	Type=Con	Read=―
：	Type=Con	Read=：
／	Type=Con	Read=／
（	Type=Con	Read=（		Sound=（
）	Type=Con	Read=）		Sound=）
、	Type=Con	Read=、
;;;"－"などの読み方制御
ノ	Type=Con	Read=ノ	Sound=ノ	; 棒読みで"ノ"と読ます場合
;ノ	Type=Con	Read=（	Sound=（	; 棒読みで休止とする場合

;MAX=10
[桁区切り]
*;*;!〇-2J'、#3j/!〇-2J'、#3j+1u/;!％;*;
*;*;!〇+4J'、#3j/!〇+4J'、#3j+1u/;!％;*;
*;*;!〇#3j'、#3j/!〇#3J'、#3j+1u/;'票;*;
*;*;!〇#3j'、〇〇〇/!〇#3J'、〇〇〇#1u/;*;*;
*;*;!〇#1j#1u#1j'、#3j/!〇#1j#1u#1j'、#3j+1u/;!％;*;
*;*;!０-3n'、#3n'、#3n/!０-3n'、#3n'、#3n+1u/!０-3n'、#3n/!０-3n'、#3n+1u/;@;*; ;;助数詞がある場合に限定

;MAX=50
[パターン]
ドル．セント=*;*;n'．#2n;'＄;*;
前置通貨='＄/'ＵＳ＄/'ＵＳＤ/'ＡＵＤ/'Ａ＄/'ＣＡＤ/'Ｃ＄/'Ｓ＄/'￥/'ＪＰＹ/'ＨＫＤ/'ＣＮＹ/;0;n'．#2n/n'．#1n/n/!０N'，#3n/!０N'，#3n'．n/!０-3n'，#3n'，#3n/;0/'札/;*; ;;接頭詞-数接続 で辞書登録が必要
整数から整数=*;*;B'－B;'シーズン;*; ;シーズンは優先
少数=*;*;!〇J'・J/!〇J'．J/!〇J'点J/!０N'．N/!０N'・N/;*;*;
少数=*;*;'〇・J/'〇．J/'〇点J/'０．N/'０・N/;*;*;
少数=*;*;N'．N'．N/;*;*; ;;version番号など
桁区切り=*;*;!０N'，#3n'，#3n'．n/!０N'，#3n'．n/!０N'，#3n'．n+1u/;*;*; ;桁数追加
桁区切り=*;*;!０N'，#3n'―!０N'，#3n/;*;*;
桁区切り=*;*;!０-3n'，#3n'，#3n/!０-3n'，#3n'，#3n+1u/;*;*; ;;100,000,000
桁区切り=*;*;!０-3n'，#3n'，#3n'，#3n;*;*; ;;100,000,000,000
桁区切り=*;*;!０-3n'，#3n'，#3n'，#3n'，#3n;*;*; ;;100,000,000,000,000
;桁区切り=*;*;!０#1n'，#3n'，#3n'，#3n'，#3n'，#3n;*;*; ;;1,000,000,000,000,000
桁区切り=*;*;!０-3n'，#3n'，#3n'，#3n'，#3n'，#3n;*;*; ;;100,000,000,000,000,000
桁区切り=*;*;!０-3n'，#3n'，#3n'，#3n'，#3n'，#3n'，#3n;*;*; ;;100,000,000,000,000,000,000 //1垓
桁区切り=*;*;!０-3n'，#3n'，#3n'，#3n'，#3n'，#3n'，#3n'，#3n;*;*; ;;100,000,000,000,000,000,000,000 //1000垓
桁区切り=*;*;!０N'，#3n/!０N'，#3n+1u/;*;*;
